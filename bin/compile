#!/usr/bin/env bash

indent() {
  sed -u 's/^/       /'
}

echo "-----> Found a hello.txt"

DOWNLOADS="/assets/downloads/"
FONTS="/assets/fonts/"
GENERICONS="/assets/genericons/"
IMAGES="/assets/images/"
VIDEOS="/assets/videos/"

AZURE_DOWNLOADS=$(cat Downloads)
AZURE_FONTS=$(cat Fonts)
AZURE_GENERICONS=$(cat Genericons)
AZURE_IMAGES=$(cat Images)
AZURE_VIDEOS=$(cat Videos)

# replace hello with goodbye in a new file
# find . -type f -exec sed -i 's+/assets\///+g' {} +

echo $AZURE_FONTS

exit 0
